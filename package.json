{
  "displayName": "Homebridge Tuya Camera",
  "name": "homebridge-tuya-cam",
  "version": "0.1.1",
  "description": "The [Homebridge](https://homebridge.io) Tuya Camera plugin allows you to get the rtsp or hls stream for your Tuya Cameras.",
  "author": "imnebo",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/imnebo/homebridge-tuya-cam.git"
  },
  "bugs": {
    "url": "https://github.com/imnebo/homebridge-tuya-cam/issues"
  },
  "engines": {
    "homebridge": "^1.4.1",
    "node": "^14.18.2 || ^16.13.1"
  },
  "main": "dist/index.js",
  "scripts": {
    "check": "npm install && npm outdated",
    "update": "ncu -u && npm update && npm install",
    "lint": "eslint src/**.ts",
    "watch": "npm run build && sudo npm link && nodemon",
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "keywords": [
    "homebridge-plugin",
    "tuya-camera",
    "tuya camera",
    "tuya",
    "camera",
    "rtsp",
    "hls"
  ],
  "dependencies": {
    "@homebridge/plugin-ui-utils": "^0.0.19",
    "axios": "^0.27.2",
    "crypto-js": "^4.1.1",
    "rxjs": "^7.5.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^17.0.36",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "homebridge": "^1.4.1",
    "nodemon": "^2.0.16",
    "npm-check-updates": "^13.0.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2",
    "typescript-axios-wb": "^1.0.3"
  }
}
